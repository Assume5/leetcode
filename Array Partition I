/**
 * @param {number[]} nums
 * @return {number}
 */
var arrayPairSum = function(nums) {
    nums=nums.sort(function(a,b){return a-b;});
    let sum=0;
    for(let i=0;i<nums.length;i+=2){
        if(nums[i]>nums[i+1]){
            sum+=nums[i+1];
        }     
        else{
            sum+=nums[i]
        }
    }
    return sum;
};
